{
    "name": "worldbreakersdb",
    "description": "This repository holds the source code of [WorldbreakersDB](https://worldbreakersdb.com).",
    "files": [
        "web/dist"
    ],
    "main": "./web/dist/worldbreakersdb.umd.js",
    "module": "./web/dist/worldbreakersdb.es.js",
    "exports": {
        ".": {
            "import": "./web/dist/worldbreakersdb.es.js",
            "require": "./web/dist/worldbreakersdb.umd.js"
        }
    },
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "format": "prettier --write --plugin-search-dir=. ./**/*.svelte",
        "preview": "vite preview"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/worldbreakers/worldbreakersdb.git"
    },
    "author": "tessaluniki",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/worldbreakers/worldbreakersdb/issues"
    },
    "homepage": "https://github.com/worldbreakers/worldbreakersdb#readme",
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.41",
        "cssnano": "^5.1.7",
        "postcss": "^8.4.12",
        "postcss-import": "^14.1.0",
        "postcss-preset-env": "^7.4.3",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "vite": "^2.9.1",
        "vite-plugin-eslint": "^1.4.0"
    }
}
